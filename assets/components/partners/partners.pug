mixin partners()
    +b.partners
        .container.mb-20.mb-sm-30.mb-lg-40.pb-lg-20
            +e.inner.row
                .col-offset-1.col-10.col-sm-10.col-md-10.col-lg-10
                    +e.container.swiper-container
                        +e.wrapper.swiper-wrapper
                            +e.slide.swiper-slide
                                +e.link(href="#")
                                    +e.img(src=require("./../images/alfa.svg"), alt="alfa")
                            +e.slide.swiper-slide
                                +e.link(href="#")
                                    +e.img(src=require("./../images/mtb.svg"), alt="mtb")
                            +e.slide.swiper-slide
                                +e.link(href="#")
                                    +e.img(src=require("./../images/status.svg"), alt="status")
                            +e.slide.swiper-slide
                                +e.link(href="#")
                                    +e.img(src=require("./../images/bate.svg"), alt="bate")
                            +e.slide.swiper-slide
                                +e.link(href="#")
                                    +e.img(src=require("./../images/bsb.svg"), alt="bsb")
                            +e.slide.swiper-slide
                                +e.link(href="#")
                                    +e.img(src=require("./../images/status.svg"), alt="status")
            +e.button-next.swiper-button-next
            +e.button-prev.swiper-button-prev
